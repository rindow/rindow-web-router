{
    "name": "rindow/rindow-web-router",
    "type": "library",
    "description": "Rindow web routing engine",
    "keywords": ["rindow","framework","web","routing"],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "interop-phpobjects/lenient-web": "*",
        "rindow/rindow-stdlib": "1.*"
    },
    "require-dev": {
        "interop-phpobjects/lenient-web": "*",
        "rindow/rindow-stdlib": "1.*",
        "rindow/rindow-web-http": "1.*",
        "rindow/rindow-container": "1.*",
        "rindow/rindow-annotation": "1.*",
        "rindow/rindow-module-yaml": "1.*"
    },
    "suggest": {
        "rindow/rindow-container": "It needs some service container",
        "rindow/rindow-web-http": "Default http message library",
        "rindow/rindow-annotation": "When using annotations",
        "rindow/rindow-module-yaml": "When using Yaml"
    },
    "autoload": {
        "psr-0": {
            "Rindow\\Web\\Router\\": "src/"
        }
    }
}
